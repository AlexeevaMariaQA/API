				{
					"name": "200 OK -  Get a product list",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"pm.test(\"Время ответа меньше 1000 мс\", function () {",
									"    pm.expect(pm.response.responseTime).to.be.below(1000);",
									"});",
									"pm.test(\"Проверка структуры и типов для каждого продукта\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.be.an(\"array\");",
									"    jsonData.forEach(function(product) {",
									"        pm.expect(product).to.have.property(\"product_id\").that.is.a(\"number\");",
									"        pm.expect(product).to.have.property(\"name\").that.is.a(\"string\");",
									"        pm.expect(product).to.have.property(\"description\").that.is.a(\"string\");",
									"        pm.expect(product).to.have.property(\"price\").that.is.a(\"string\");",
									"        pm.expect(product).to.have.property(\"category\").that.is.a(\"string\");",
									"        pm.expect(product).to.have.property(\"manufacturer\").that.is.a(\"string\");",
									"        pm.expect(product).to.have.property(\"imageUrl\").that.is.a(\"string\");",
									"        pm.expect(product).to.have.property(\"freeShipping\");",
									"        pm.expect(product.freeShipping === 0 || product.freeShipping === 1, `freeShipping should be 0 or 1, got ${product.freeShipping}`).to.be.true;",
									"    });",
									"});",
									"",
									"pm.test(\"Цена в формате строки с двумя десятичными знаками\", function () {",
									"    var jsonData = pm.response.json();",
									"    jsonData.forEach(function(product) {",
									"        pm.expect(product.price).to.match(/^\\d+(\\.\\d{2})$/);",
									"    });",
									"});",
									""
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
